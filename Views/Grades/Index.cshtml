@model IEnumerable<SchoolProject.Data.Grade>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
          
            <th>
                @Html.DisplayNameFor(model => model.ActivityMark)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FinalExam)
            </th>
            
            <th>
                @Html.DisplayNameFor(model => model.GradeStudent)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GradeCourse)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.ActivityMark)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FinalExam)
            </td>
           
            <td>
                @Html.DisplayFor(modelItem => item.GradeStudent.StudentId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GradeCourse.CourseName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.GradeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.GradeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.GradeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
